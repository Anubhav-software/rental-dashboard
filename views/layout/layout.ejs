<!-- meta tags and other links -->
<!DOCTYPE html>
<html lang="en" data-theme="light">
  <%- include('../partials/head') %>
  <body>
  <% if (typeof basePath !== 'undefined' && basePath === '/staff') { %>
    <%- include('../partials/staff_sidebar') %>
  <% } else { %>
    <%- include('../partials/owner_sidebar') %>
  <% } %>

    <main class="dashboard-main">
      <%- include('../partials/navbar') %>

        <div class="dashboard-main-body">
            <%- include('../partials/breadcrumb'), {title:title, subTitle:subTitle} %> 

            <%- body %>
        </div>

      <%- include('../partials/footer') %>
    </main>
    <script src="/js/config.js"></script>
    <script src="/js/api/authApi.js"></script>
    <script src="/js/api/companyApi.js"></script>
    <script src="/js/api/authGuard.js"></script>
    <script src="/js/api/sidebarControl.js"></script>
    <script src="/js/api/companyLogo.js"></script>
    <%- include('../partials/scripts') %> 

   <!-- Add the script block here. If script does not end with </script>, close the tag (for view/edit vehicle inline scripts). -->
   <% if (typeof script !== 'undefined') { %>
    <%- script %><% if (typeof script === 'string' && !/<\/script>\s*$/.test(script)) { %></script><% } %>
  <% } %>
  </body>
</html>
